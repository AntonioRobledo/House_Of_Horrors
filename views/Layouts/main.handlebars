<table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Release Date</th>
      </tr>
    </thead>
    <tbody>
      {{#each movies}}
        <tr>
          <td>{{this.title}}</td>
          <td>{{this.release_date}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>